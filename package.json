{
  "name": "agent-capability-broker",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "postinstall": "prisma generate",
    "lint:duplication": "jscpd src --threshold 5",
    "test:coverage": "vitest run --coverage --coverage.thresholds.lines=80 --coverage.thresholds.branches=80",
    "quality": "npm run lint && npm run lint:duplication && npm run test:coverage"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.1.0",
    "@prisma/client": "^7.1.0",
    "next": "16.0.10",
    "pg": "^8.16.3",
    "prisma": "^7.1.0",
    "react": "19.2.1",
    "react-dom": "19.2.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20.19.27",
    "@types/pg": "^8.16.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.1.2",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "jscpd": "^4.0.5",
    "jsdom": "^27.3.0",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "vitest": "^4.0.15"
  }
}
